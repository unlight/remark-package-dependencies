{
  "name": "remark-package-dependencies",
  "version": "1.1.0",
  "description": "Inject to markdown the list of dependencies of your package",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test:d": "node --inspect-brk node_modules/mocha/bin/mocha index.spec.js",
    "test:w": "npm run test -- --watch index.spec.js index.js",
    "eslint": "eslint index.js",
    "eslint:w": "watchexec -w . \"npm run eslint\"",
    "test": "node node_modules/mocha/bin/mocha index.spec.js"
  },
  "dependencies": {
    "bundle-phobia-cli": "^0.14.1",
    "njct": "^8.0.0",
    "prettysize": "^2.0.0",
    "unist-util-visit": "^2.0.1"
  },
  "peerDependencies": {
    "remark": ">=10"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "eslint": "^6.8.0",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "remark": "^11.0.1",
    "watchexec-bin": "^1.0.0",
    "semantic-release": "^15.14.0"
  },
  "keywords": [
    "remark-plugin",
    "markdown",
    "remark",
    "dependencies"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/remark-package-dependencies.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/remark-package-dependencies/issues"
  },
  "homepage": "https://github.com/unlight/remark-package-dependencies#readme"
}
